@section scripts{
    <script>
        window.fbAsyncInit = function () {
            FB.init({
                appId: '1753687224853096',
                cookie: true,
                xfbml: true,
                version: 'v2.2',
                status: true
            });

            //FB.getLoginStatus(function (response) {
            //    statusChangeCallback(response);
            //});
        };

        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/pt_BR/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));

        function logar() {
            login = {
                Email: $('#txtEmail').val(),
                Senha: $('#txtSenha').val()
            }
            $.ajax({
                url: '/Home/LogarUsuario',
                type: "POST",
                contentType: "application/json",
                data: JSON.stringify(login),
                success: function (result) {
                    if (result.info == "redirect") { window.location.href = result.data; }
                }
            });
        }

        function logarViaFacebook() {
            FB.login(
                function (response) {
                    if (response.authResponse) {
                        FB.api('/me?fields=id,name,email,permissions', function (accountResponse) {
                            facebookAccount = {
                                Id: accountResponse.id,
                                Nome: accountResponse.name,
                                Email: accountResponse.email,
                                AccessToken: response.authResponse.accessToken
                            }
                            $.ajax({
                                url: '/Home/LogarPeloFacebook',
                                type: "POST",
                                contentType: "application/json",
                                data: JSON.stringify(facebookAccount),
                                success: function (result) {
                                    switch (result.info) {
                                        case "solicitarDefinicaoPerfil":
                                            $("#divDefinicaoPerfil").show();
                                            break;
                                        case "redirect":
                                            window.location.href = result.data;
                                            break;
                                        case "exigirSenhaEmailEncontrado":
                                            $("#divEmailEncontrado").show();
                                            break;
                                    }
                                }
                            });
                        });
                    } else {
                        console.log('User cancelled login or did not fully authorize.');
                    }
                },
                { scope: 'public_profile,email' }
            );
        }

        function cadastrarUsuario(perfil) {
            if ($.trim($("#txtNomeCadastro").val()) === "") {
                alert('Tem que preencher o nome, cabeção!');
                return;
            }

            if ($("#slctEstado").val() == null) {
                alert('Tem que escolher o estado, cabeção!');
                return;
            }

            if ($.trim($("#txtEmailCadastro").val()) === "") {
                alert('Tem que preencher o email, cabeção!');
                return;
            }

            senha = $("#txtSenhaCadastro").val();
            confirmacaoSenha = $("#txtConfirmacaoSenhaCadastro").val();
            if (senha !== confirmacaoSenha) {
                alert('olha essa senha ai rapá! A confirmação tá diferente!');
                return;
            }
            cadastro = {
                Nome: $("#txtNomeCadastro").val(),
                Sobrenome: $("#txtSobrenomeCadastro").val(),
                Email: $("#txtEmailCadastro").val(),
                Senha: $("#txtSenhaCadastro").val(),
                ConfirmacaoSenha: $("#txtConfirmacaoSenhaCadastro").val()
            }
            if (perfil == 'particular') {
                urlCadastro = '/Home/CadastrarUsuarioParticular';
            } else if (perfil == 'profissional') {
                urlCadastro = '/Home/CadastrarUsuarioProfissional';
            }
            $.ajax({
                url: urlCadastro,
                type: "POST",
                contentType: "application/json",
                data: JSON.stringify(cadastro),
                success: function (result) {
                    if (result.info == 'redirect') {
                        window.location.href = result.data;
                    }
                }
            });
        }
    </script>
}

